{
  "name": "@davidtranjs/webpack-log-forward-plugin",
  "version": "1.0.3",
  "description": "A webpack plugin that forwards browser console logs to terminal during development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "biome check .",
    "gen-doc": "node gen-doc.js",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "preinstall": "npx only-allow pnpm"
  },
  "packageManager": "pnpm@10.12.0",
  "keywords": [
    "webpack",
    "plugin",
    "console",
    "log",
    "forward",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/webpack": "^5.28.0",
    "jest": "^29.0.0",
    "marked": "16.1.1",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davidtranjs/webpack-log-forward-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/davidtranjs/webpack-log-forward-plugin/issues"
  },
  "homepage": "https://github.com/davidtranjs/webpack-log-forward-plugin#readme"
}
